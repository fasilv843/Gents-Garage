    <!-- Contact Section Begin -->
    <section class="contact spad" id="contactUs">
        <div class="container">
            <div class="row">
                <div class="col-lg-6 col-md-6">
                    <div class="contact__text">
                        <div class="section-title">
                            <span>Information</span>
                            <h2>Contact Us</h2>
                            <p>As you might expect of a company that began as a high-end interiors contractor, we pay
                                strict attention.</p>
                        </div>
                        <ul>
                            <li>
                                <h4>Malappuram</h4>
                                <p>Vengara, Kerala, India <br />+91 8129 243 400</p>
                            </li>
                            <li>
                                <h4>Calicut</h4>
                                <p>Kozhikkode, Kerala, India <br />+91 8129 243 500</p>
                            </li>
                            <li>
                                <h4>Kochi</h4>
                                <p>Kochi, Kerala, India <br />+91 8129 243 600</p>
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-6 col-md-6">
                    <div class="contact__form">
                        <form action="#">
                            <div class="row">
                                <div class="col-lg-6">
                                    <input type="text" name="fullname" placeholder="Enter Name">
                                </div>
                                <div class="col-lg-6">
                                    <input type="text" name="email" placeholder="Enter Email">
                                </div>
                                <div class="col-lg-12">
                                    <textarea placeholder="Message" name="message"></textarea>
                                    <button type="submit" class="site-btn">Send Message</button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Contact Section End -->


